Algoritmo "Nome e data de Nascimento"

Var
   nome: caractere
   dataNascimento: caractere


Inicio
      escreval("Digite seu nome: ")
      leia(nome)
      escreval("Digite sua data de Nascimento:")
      leia(dataNascimento)
      escreva("Seu nome é:",nome," e você nasceu em:",dataNascimento)
Fimalgoritmo