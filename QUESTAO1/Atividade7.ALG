Algoritmo "conversao de real para dolar"

Var
   dolar, valorConversao, resultado: real
Inicio
      escreval("informe o valor do dolar hoje: ")
      leia(dolar)
      escreval("informe o valor em reais que deseja converter: ")
      leia(valorConversao)
      resultado <- valorConversao / dolar
      escreva("valor da conversao: ",resultado)
Fimalgoritmo